{
  "typescript.check.workspaceVersion": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "bin": true,
    "obj": true,
    "dist": true,
    "Properties": true,
    ".vs": true,
    "*.csproj": true,
    "*.sln": true,
    "*.csproj.user": true,
    "*.suo": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
