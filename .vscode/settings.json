{
  "typescript.check.workspaceVersion": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "bin": true,
    "obj": true,
    "dist": true,
    "Properties": true,
    ".vs": true,
    "*.csproj": true,
    "*.sln": true,
    "*.csproj.user": true,
    "*.suo": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#01BBF9",
    "activityBar.activeBorder": "#FFD803",
    "activityBar.background": "#01BBF9",
    "activityBar.foreground": "#000",
    "activityBar.inactiveForeground": "#000",
    "activityBarBadge.background": "#FFD803",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#01BBF9",
    "statusBar.background": "#01BBF9",
    "statusBar.foreground": "#ffffff",
    "statusBarItem.hoverBackground": "#01BBF9",
    "statusBarItem.remoteBackground": "#01BBF9",
    "statusBarItem.remoteForeground": "#000",
    "titleBar.activeBackground": "#01BBF9",
    "titleBar.activeForeground": "#000",
    "titleBar.inactiveBackground": "#01BBF9",
    "titleBar.inactiveForeground": "#000",
    "commandCenter.border": "#000"
  }
}
